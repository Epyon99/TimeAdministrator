{
  "persistence": {
    "type": "mongodb",
    "host": "epyhost",
    "port": 27017,
    "usessl": false,
    "db": "storage",
    "collection": "TipoLogTiempos"
  },
  "authentication": {
    "Authority": "http://epyhost:6001",
    "RequireHttpsMetadata": false,
    "EnableCaching": false,
    "ApiName": "api",
    "ApiSecret": "secret"
  },
  "kafka": {
    "bootstrapservers": [ "epyhost:9092" ],
    "topics": [ "system.events" ],
    "consumergroup": "services.timelog"
  }
}